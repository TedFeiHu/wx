        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>
    <style name="contacts_im">
        <item name="android:layout_width">48dp</item>
        <item name="android:layout_height">48dp</item>
        <item name="android:layout_margin">6dp</item>
    </style>
    <style name="contacts_tv">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">60dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:background">@drawable/me_press_back</item>
        <item name="android:clickable">true</item>
        <item name="android:paddingLeft">60dp</item>
        <item name="android:layout_margin">0.3dp</item>
    </style>
    <style name="im_rigent_sty">
        <item name="android:layout_width">18dp</item>
        <item name="android:layout_height">18dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_alignParentRight">true</item>
        <item name="android:layout_marginRight">8dp</item>
    </style>
    <style name="im_sty">
        <item name="android:layout_width">28dp</item>
        <item name="android:layout_height">28dp</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:layout_centerVertical">true</item>
    </style>
    <style name="info_id_tv">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:textSize">16sp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:singleLine">true</item>
        <item name="android:paddingRight">10dp</item>
        <item name="android:background">@color/white</item>
    </style>
    <style name="info_tv">
        <item name="android:layout_width">94dp</item>
